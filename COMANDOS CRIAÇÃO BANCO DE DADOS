-- USUARIO
CREATE TABLE USUARIO (
ID INTEGER PRIMARY KEY,
NOME_USUARIO VARCHAR(50),
SENHA VARCHAR(60)
);
-- SEQUENCE PARA ALIMENTAR A PRIMARY KEY DA TABELA USUARIO. 
CREATE SEQUENCE SEQ_ID_USUARIO
  START 1
  INCREMENT 1;
  
  
  
